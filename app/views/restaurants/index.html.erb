<% @restaurants.each do |restaurant| %>
  <div class="restaurantalignment">
    <h1>
      <%=link_to restaurant.name, restaurant%>
    </h1>
    <%=link_to image_tag(restaurant.picture_url, width: 200), restaurant if restaurant.picture.present?%>
  </div>

<%end%>

<%= link_to 'New Restaurant', new_restaurant_path %>
